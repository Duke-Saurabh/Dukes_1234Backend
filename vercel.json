{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "cors": {
    "origin":"*"
    },
    "commands": {
      "build": "exit 1"
    }
  }
  